{
  "builders": [
    {
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter><wait>"
      ],
      "boot_wait": "10s",
      "cpus": "{{ user `vm_cpu_num` }}",
      "disk_size": "{{user `disk_size`}}",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "guest_os_type": "{{user `guest_os_type`}}",
      "hard_drive_interface": "{{user `hard_drive_interface`}}",
      "headless": "{{user `headless`}}",
      "http_directory": "http",
      "iso_checksum": "{{user `iso_checksum_type`}}:{{user `iso_checksum`}}",
      "iso_url": "{{user `iso_url`}}",
      "memory": "{{ user `memory` }}",
      "shutdown_command": "echo '/sbin/halt -h -p' > shutdown.sh; echo 'vagrant'|sudo -S bash 'shutdown.sh'",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_pty": true,
      "ssh_timeout": "60m",
      "ssh_username": "vagrant",
      "type": "virtualbox-iso",
      "virtualbox_version_file": ".vbox_version",
      "vm_name": "{{user `vm_build_id`}}"
    }
  ],
  "post-processors": [
    [
      {
        "compression_level": "{{user `compression_level`}}",
        "type": "vagrant"
      },
      {
        "output": "manifest.json",
        "strip_path": true,
        "type": "manifest"
      }
    ]
  ],
  "provisioners": [
    {
      "override": {
        "virtualbox-iso": {
          "execute_command": "echo 'vagrant'|sudo -S bash '{{.Path}}'"
        }
      },
      "scripts": [
        "_common/scripts/base.sh",
        "_common/scripts/virtualbox.sh",
        "_common/scripts/vagrant.sh",
        "_common/scripts/cleanup.sh",
        "_common/scripts/zerodisk.sh"
      ],
      "type": "shell"
    }
  ],
  "variables": {
    "compression_level": "6",
    "disk_size": "36864",
    "guest_os_type": "RedHat_64",
    "hard_drive_interface": "sata",
    "headless": "false",
    "iso_checksum_type": "none",
    "iso_checksum": "f90e4d28fa377669b2db16cbcb451fcb9a89d2460e3645993e30e137ac37d284",
    "iso_url": "rhel-server-7.2-x86_64-dvd.iso",
    "memory": "1024",
    "vm_cpu_num": "1",
    "vm_name": "packer-rhel-7-server-x86-64"
  }
}
