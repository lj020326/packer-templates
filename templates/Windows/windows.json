{
  "builders": [
    {
      "boot_wait": "2m",
      "communicator": "winrm",
      "cpus": "{{ user `vm_cpu_num` }}",
      "disk_size": "{{ user `disk_size` }}",
      "floppy_files": [
        "{{ user `autounattend` }}",
        "_common/scripts/bootstrap.ps1",
        "_common/scripts/windows_updates.ps1"
      ],
      "guest_additions_mode": "attach",
      "guest_os_type": "{{ user `virtualbox_guest_type` }}",
      "hard_drive_interface": "{{ user `disk_adapter_type` }}",
      "headless": true,
      "iso_checksum": "{{ user `iso_checksum_type` }}:{{ user `iso_checksum` }}",
      "iso_url": "{{ user `iso_url` }}",
      "memory": "{{ user `memory` }}",
      "output_directory": "output-{{ user `vm_name` }}-{{ build_type }}-{{ timestamp }}",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
      "type": "virtualbox-iso",
      "vm_name": "{{ user `vm_name` }}-{{ timestamp }}",
      "winrm_insecure": true,
      "winrm_password": "{{ user `winrm_password` }}",
      "winrm_timeout": "8h",
      "winrm_use_ssl": false,
      "winrm_username": "{{ user `winrm_username` }}"
    },
    {
      "boot_wait": "2m",
      "communicator": "winrm",
      "cpus": "{{ user `vm_cpu_num` }}",
      "disk_adapter_type": "{{ user `disk_adapter_type` }}",
      "disk_size": "{{ user `disk_size` }}",
      "disk_type_id": 0,
      "floppy_files": [
        "{{ user `autounattend` }}",
        "_common/scripts/bootstrap.ps1",
        "_common/scripts/windows_updates.ps1"
      ],
      "guest_os_type": "{{ user `vmware_guest_type` }}",
      "headless": true,
      "iso_checksum": "{{ user `iso_checksum_type` }}:{{ user `iso_checksum` }}",
      "iso_url": "{{ user `iso_url` }}",
      "memory": "{{ user `memory` }}",
      "output_directory": "output-{{ user `vm_name` }}-{{ build_type }}-{{ timestamp }}",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
      "tools_upload_flavor": "windows",
      "tools_upload_path": "c:/Windows/Temp/windows.iso",
      "type": "vmware-iso",
      "version": 11,
      "vm_name": "{{ user `vm_name` }}-{{ timestamp }}",
      "vmx_data": {
        "scsi0.virtualDev": "lsisas1068"
      },
      "vmx_remove_ethernet_interfaces": true,
      "winrm_insecure": true,
      "winrm_password": "{{ user `winrm_password` }}",
      "winrm_timeout": "8h",
      "winrm_use_ssl": false,
      "winrm_username": "{{ user `winrm_username` }}"
    }
  ],
  "post-processors": [
    [
      {
        "compression_level": "{{ user `compression_level` }}",
        "output": "{{ user `vm_name` }}-{{.Provider}}-{{ timestamp }}.box",
        "type": "vagrant"
      },
      {
        "access_token": "{{ user `vagrant_cloud_token` }}",
        "box_tag": "{{ user `box_tag` }}",
        "type": "vagrant-cloud",
        "version": "{{ timestamp }}"
      },
      {
        "output": "manifest.json",
        "strip_path": true,
        "type": "manifest"
      }
    ]
  ],
  "provisioners": [
    {
      "type": "windows-restart"
    },
    {
      "scripts": [
        "_common/scripts/base.ps1",
        "_common/scripts/remove_default_apps.ps1",
        "_common/scripts/virtualbox.ps1",
        "_common/scripts/vmware.ps1",
        "_common/scripts/cleanup.ps1"
      ],
      "type": "powershell"
    }
  ],
  "variables": {
    "autounattend": "",
    "boot_command_prefix": "",
    "box_tag": "",
    "compression_level": "6",
    "disk_adapter_type": "sata",
    "disk_size": "36864",
    "iso_checksum_type": "",
    "iso_checksum": "",
    "iso_url": "",
    "memory": "2048",
    "vagrant_cloud_token": "",
    "vm_cpu_num": "2",
    "virtualbox_guest_type": "",
    "vm_name": "",
    "winrm_password": "vagrant",
    "winrm_username": "vagrant",
    "vmware_guest_type": ""
  }
}
